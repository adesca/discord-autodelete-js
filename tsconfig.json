{
	"compilerOptions": {
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "target": "es2022",
	  "allowJs": true,
	  "resolveJsonModule": true,
	  "moduleDetection": "force",
	  "module": "Preserve",
	  "isolatedModules": true,
	  "verbatimModuleSyntax": false,
  
	  "strict": true,
	  "noUncheckedIndexedAccess": true,
	  "noImplicitOverride": true,
  

	
	  "outDir": "dist",
	  "sourceMap": true,
  
	  "lib": ["es2022"]
	}
  }